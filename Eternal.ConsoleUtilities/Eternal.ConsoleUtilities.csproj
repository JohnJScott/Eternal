<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Helper functions for logging, launching processes, reading and writing Json and Xml files.</Description>
    <Copyright>Copyright 2022 Eternal Developments, LLC. All Rights Reserved.</Copyright>
    <PackageProjectUrl>https://eternaldevelopments.com</PackageProjectUrl>
    <PackageReadmeFile>ReadMe.md</PackageReadmeFile>
    <RepositoryType>Perforce</RepositoryType>
    <PackageTags>json; xml; logging</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <Company>Eternal Developments, LLC.</Company>
    <Authors>JohnScott</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Update="ReadMe.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
